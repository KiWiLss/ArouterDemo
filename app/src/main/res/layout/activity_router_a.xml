        android:layout_height="wrap_content">

        <Button
            android:id="@+id/btnRouterANo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无参跳转"/>

        <Button
            android:id="@+id/btnRouterAHas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="带参跳转"/>

        <Button
